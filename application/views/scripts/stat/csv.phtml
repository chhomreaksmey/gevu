<?php 
	//echo $this->filename;
	// send response headers to the browser
	header( 'Content-Type: text/csv' );
	header( 'Content-Disposition: attachment;filename='.$this->filename);
	$fp = fopen('php://output', 'w');
  	
	$row = $this->stats[0];
	if($row) {
		fputcsv($fp, array_keys($row));
	}
	 
	foreach ($this->stats as $row) {
		fputcsv($fp, $row);
	}
 
	fclose($fp);	
